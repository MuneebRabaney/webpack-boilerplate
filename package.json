{
  "name": "webpack-boilerplate",
  "version": "0.1.0",
  "description": "Webpack and bable 7 boilerplate with react",
  "repository": "https://github.com/MuneebRabaney/webpack-boilerplate",
  "main": "./src/index.js",
  "scripts": {
    "lint": "eslint",
    "pretty": "prettier --write \"src/**/*.{jsx,js}\"",
    "storybook": "start-storybook -p 9090 -c .storybook",
    "server": "json-server --watch json-server/db.json --port 3333",
    "start": "webpack-dev-server --config ./config/webpack.development.config.js --colors --mode=development ",
    "build:staging": "webpack --mode=production --config ./config/webpack.production.config.js --progress --colors",
    "build:production": "webpack --mode=production --config ./config/webpack.production.config.js --progress --colors"
  },
  "keywords": [
    "react",
    "babel 7",
    "Webpack",
    "boilderplate"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-preset-airbnb": "^4.0.1",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^3.2.1",
    "css-loader": "^3.2.0",
    "custom-event-polyfill": "^1.0.7",
    "dotenv": "^8.1.0",
    "dotenv-webpack": "^1.7.0",
    "error-overlay-webpack-plugin": "^0.4.2",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "styled-components": "^4.3.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
